resources:
- type: compute.v1.network
  name: {{ properties["name"] }}
  properties:
     autoCreateSubnetworks: false
- type: compute.v1.subnetwork
  name: {{ properties["name"] }}
  properties:
    network: $(ref.{{ properties["name"] }}.selfLink)
    ipCidrRange: {{ range }}
    region: {{ properties["region"] }}